<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="mainTitle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">32sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="subTitle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:gravity">right</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="thirdTitle">
        <item name="android:layout_width">120sp</item>
        <item name="android:textSize">14dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
    </style>
    
    <style name="midTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">left</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>
    
    <style name="generalEditText">
        <item name="android:ems">10</item>
        <item name="android:background">@drawable/edit_text_holo_dark</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:textSize">14dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
    </style>

    
    <style name="generalEditTextReadonly" parent="generalEditText">
        <item name="android:editable">false</item>
    </style>
    
    
    <style name="horizontalRight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:baselineAligned">true</item>
        <item name="android:layout_gravity">right</item>
        <item name="android:gravity">right</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">25dp</item>
    </style>

    <style name="horizontalCenter">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:baselineAligned">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">0dp</item>
    </style>
    
    <style name="horizontalLeft">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:baselineAligned">true</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">0dp</item>
    </style>
    
    <style name="myRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ic_item</item>
        <item name="android:minHeight">26dip</item>
        <item name="android:maxHeight">26dip</item>
    </style>

    <style name="generalCheckButtonStyle">
         <item name="android:button">@drawable/btn_check_holo_light</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        
    </style>
    
    
    <style name="generalButtonStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/btn_default_holo_dark</item>
        
    </style>
    <!-- <item name="android:textColor">@color/submitbtn_color</item> -->
    <!-- more -->

    <style name="TextViewStyleTabMainNav">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textColor">@drawable/tab_main_nav_text_selector</item>
    </style>

    <style name="listItemStyle">
        <item name="android:background">@drawable/listview_item</item>
    </style>

    <style name="TextViewStyleHeaderButtonBlue">
        <item name="android:textSize">17.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black_end</item>
    </style>

    <style name="TextViewStyleHeaderDivider">
        <item name="android:textSize">21.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/grey_end</item>
    </style>
    <style name="TextViewStyleCheckinListItemLineMessage1">
        <item name="android:textSize">13.0dip</item>
        <item name="android:textColor">@color/grey_end</item>
    </style>

    <style name="TextViewStyleCheckinListItemLineMessageBigger1">
        <item name="android:textSize">15.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/grey_end</item>
    </style>
    <style name="TextViewStyleCheckinListItemLineMessage">
        <item name="android:textSize">13.0dip</item>
        <item name="android:textColor">@color/black_end</item>
    </style>

    <style name="TextViewStyleCheckinListItemLineMessageBigger">
        <item name="android:textSize">15.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black_end</item>
    </style>

    <style name="TextViewStyleCheckinListItemLineAddress">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textColor">@color/grey_end</item>
    </style>

    <style name="TextViewStyleCheckinListItemLineTimestamp">
        <item name="android:textSize">13.0dip</item>
        <item name="android:textColor">@color/grey_end</item>
    </style>

    <style name="TextViewStyleUserDetailsActivityTitle">
        <item name="android:textSize">20.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/grey_end</item>
    </style>

    <style name="TextViewStyleUserDetailsActivityLastSeen">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextViewStyleUserDetailsActivityGeneralTitle">
        <item name="android:textSize">11.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextViewStyleUserDetailsActivityGeneralValue">
        <item name="android:textSize">18.0dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextViewStyleVenueActivityBold">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ThemeTransparentDialogBase" parent="android:Theme.Black" />

    <style name="ThemeTransparentDialogBase.TransparentDialog" parent="@style/ThemeTransparentDialogBase">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@*android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="HeaderSegment">
        <item name="android:background">@drawable/ratingbackground</item>
    </style>

    <style name="HeaderSegment.yellow" parent="@style/HeaderSegment">
        <item name="android:background">@drawable/header_yellow</item>
    </style>

    <style name="HeaderSegment.green" parent="@style/HeaderSegment">
        <item name="android:background">@drawable/header_green</item>
    </style>

    <style name="My_Theme">
        <item name="android:windowTitleSize">0.0dip</item>
    </style>

    <style name="MoreTextViewStyleCheckinListItemLineAddress">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textColor">@color/black_start</item>
    </style>

    <style name="filter_btn">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/filter_btn_clickable_deep_black</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/ic_btn_filter</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:layout_width">0.0px</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawableRight">@drawable/ic_arrow_down</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="filter_btn2">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/ic_btn_filter1</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:layout_width">0.0px</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawableRight">@drawable/ic_arrow_down</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="filter_panel">
        <item name="android:background">@color/translucent_background</item>
    </style>

    <style name="SpinnerLook" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textSize">22.0sp</item>
    </style>

    <style name="submitbuttonStyle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/submitbtn_color</item>
    </style>

    <style name="mappagecontrol">
        <item name="android:textSize">24.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="EditTextList">
        <item name="android:textSize">19.0dip</item>
        <item name="android:textColor">@color/grey_end</item>
    </style>
